def quicksort(list):
    smaller=[]
    bigger=[]
    point=[]
    if len(list)<=1:
        return list
    else:
        pivot = list[-1]
        for i in list:
            if i < pivot:
                smaller.append(i)
            elif i > pivot:
                bigger.append(i)
            else:
                point.append(i)
                
    return quicksort(smaller) + point + quicksort(bigger)
